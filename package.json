		"mode-watcher": "^1.1.0",
		"rehype-pretty-code": "^0.14.1",
		"rehype-slug": "^6.0.0",
		"remark-code-import": "^1.2.0",
		"remark-gfm": "^4.0.1",
		"unist-builder": "^4.0.0",
		"unist-util-visit": "^5.1.0"
	},
	"devDependencies": {
		"@internationalized/date": "^3.11.0",
