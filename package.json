{
	"name": "arcane-website",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"velite": "velite",
		"velite:watch": "velite --watch",
		"dev": "pnpm velite:watch & vite dev --port 3001",
		"dev:wrangler": "pnpm velite:watch & wrangler dev",
		"prebuild": "pnpm velite",
		"build": "vite build",
		"preview": "vite preview",
		"preview:wrangler": "wrangler dev --local",
		"deploy": "pnpm build && wrangler deploy",
		"deploy:dry-run": "pnpm build && wrangler deploy --dry-run",
		"prepare": "husky",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "oxfmt",
		"format:check": "oxfmt --check",
		"knip": "knip",
		"lint": "oxlint",
		"lint:fix": "oxlint --fix"
	},
	"dependencies": {
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/enhanced-img": "^0.10.3",
		"@types/js-yaml": "^4.0.9",
		"js-yaml": "^4.1.1",
		"mode-watcher": "^1.1.0",
		"rehype-pretty-code": "^0.14.1",
		"rehype-slug": "^6.0.0",
		"remark-code-import": "^1.2.0",
		"remark-gfm": "^4.0.1",
		"unist-builder": "^4.0.0",
		"unist-util-visit": "^5.1.0"
	},
	"devDependencies": {
		"@internationalized/date": "^3.11.0",
		"@lucide/svelte": "^0.575.0",
		"@shikijs/langs": "^3.22.0",
		"@shikijs/themes": "^3.22.0",
		"@sveltejs/kit": "^2.53.0",
		"@sveltejs/vite-plugin-svelte": "7.0.0-next.0",
		"@tailwindcss/vite": "^4.2.1",
		"@types/node": "^25.3.0",
		"bits-ui": "^2.16.2",
		"clsx": "^2.1.1",
		"husky": "^9.1.7",
		"isomorphic-dompurify": "^2.36.0",
		"knip": "^5.85.0",
		"mdsx": "^0.0.7",
		"oxfmt": "^0.35.0",
		"oxlint": "^1.50.0",
		"oxlint-tsgolint": "^0.14.2",
		"package-manager-detector": "^1.6.0",
		"prettier-plugin-svelte": "^3.5.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"runed": "^0.37.1",
		"shiki": "^3.22.0",
		"svelte": "^5.53.3",
		"svelte-check": "^4.4.3",
		"svelte-toolbelt": "^0.10.6",
		"tailwind-merge": "^3.5.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.2.1",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"velite": "^0.3.1",
		"vite": "8.0.0-beta.15",
		"wrangler": "^4.68.0"
	},
	"packageManager": "pnpm@10.30.2+sha512.36cdc707e7b7940a988c9c1ecf88d084f8514b5c3f085f53a2e244c2921d3b2545bc20dd4ebe1fc245feec463bb298aecea7a63ed1f7680b877dc6379d8d0cb4"
